{
  "name": "jaws-festa-memory-upload-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for JAWS FESTA Memory Upload application",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
    "deploy:dev": "npm run build && cdk deploy --context environment=dev",
    "deploy:prod": "npm run build && cdk deploy --context environment=prod",
    "diff": "npm run build && cdk diff",
    "synth": "npm run build && cdk synth",
    "destroy": "cdk destroy",
    "bootstrap": "cdk bootstrap"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.3.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "infrastructure",
    "serverless",
    "typescript"
  ],
  "author": "JAWS-UG",
  "license": "MIT"
}