{
  "name": "jaws-festa-memory-upload-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for JAWS FESTA Memory Upload application",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
    "deploy:dev": "npm run build && cdk deploy --context environment=dev --outputs-file cdk-outputs-dev.json",
    "deploy:prod": "npm run build && cdk deploy --context environment=prod --profile prod --require-approval never",
    "diff": "npm run build && cdk diff",
    "diff:dev": "npm run build && cdk diff --context environment=dev",
    "diff:prod": "npm run build && cdk diff --context environment=prod --profile prod",
    "synth": "npm run build && cdk synth",
    "synth:dev": "npm run build && cdk synth --context environment=dev",
    "synth:prod": "npm run build && cdk synth --context environment=prod",
    "destroy": "cdk destroy",
    "destroy:dev": "cdk destroy --context environment=dev",
    "destroy:prod": "cdk destroy --context environment=prod --profile prod",
    "bootstrap": "cdk bootstrap",
    "bootstrap:dev": "cdk bootstrap",
    "bootstrap:prod": "cdk bootstrap --profile prod",
    "deploy-script:dev": "bash scripts/deploy-dev.sh",
    "deploy-script:prod": "bash scripts/deploy-prod.sh",
    "deploy-script:win": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "setup:dev": "npm install && npm run bootstrap:dev && npm run deploy:dev",
    "setup:prod": "npm install && npm run bootstrap:prod && npm run deploy:prod",
    "clean": "rm -rf lib/ cdk.out/ node_modules/ *.tsbuildinfo",
    "clean:win": "rmdir /s /q lib cdk.out node_modules & del *.tsbuildinfo",
    "validate": "npm run build && npm run test && npm run synth:dev",
    "validate:prod": "npm run build && npm run test && npm run synth:prod",
    "setup-data": "node scripts/setup-initial-data.js",
    "setup-data:dev": "node scripts/setup-initial-data.js dev",
    "setup-data:prod": "node scripts/setup-initial-data.js prod",
    "setup-data:force": "node scripts/setup-initial-data.js dev --force",
    "show-config": "node scripts/setup-initial-data.js dev --show",
    "show-config:prod": "node scripts/setup-initial-data.js prod --show",
    "update-config": "node scripts/update-config.js",
    "config-template": "node scripts/update-config.js --template",
    "deploy-frontend": "node scripts/deploy-frontend.js",
    "deploy-frontend:dev": "node scripts/deploy-frontend.js dev",
    "deploy-frontend:prod": "node scripts/deploy-frontend.js prod",
    "deploy-frontend:build": "node scripts/deploy-frontend.js dev --build",
    "deploy-frontend:build-prod": "node scripts/deploy-frontend.js prod --build",
    "deploy-all:dev": "npm run deploy:dev && npm run deploy-frontend:dev --build",
    "deploy-all:prod": "npm run deploy:prod && npm run deploy-frontend:prod --build",
    "update-frontend-env:dev": "powershell -ExecutionPolicy Bypass -File ../scripts/update-frontend-env.ps1 -Environment dev",
    "update-frontend-env:prod": "powershell -ExecutionPolicy Bypass -File ../scripts/update-frontend-env.ps1 -Environment prod",
    "cloudfront:invalidate": "node scripts/cloudfront-cache-policy.js dev invalidate",
    "cloudfront:invalidate-prod": "node scripts/cloudfront-cache-policy.js prod invalidate",
    "cloudfront:status": "node scripts/cloudfront-cache-policy.js dev status",
    "cloudfront:tips": "node scripts/cloudfront-cache-policy.js dev tips"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.19.17",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.896.0",
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "aws-cdk-lib": "^2.215.0",
    "constructs": "^10.3.0",
    "mime-types": "^2.1.35"
  },
  "keywords": [
    "aws",
    "cdk",
    "infrastructure",
    "serverless",
    "typescript"
  ],
  "author": "JAWS-UG",
  "license": "MIT"
}
